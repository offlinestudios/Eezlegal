{
  "version": 2,
  "name": "eezlegal-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_BACKEND_URL": "@backend_url",
    "VITE_STRIPE_PUBLISHABLE_KEY": "@stripe_key"
  },
  "build": {
    "env": {
      "VITE_BACKEND_URL": "@backend_url",
      "VITE_STRIPE_PUBLISHABLE_KEY": "@stripe_key"
    }
  }
}
